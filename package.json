{
  "name": "cm-mumble",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "preflight": "npm run prettier && npm run lint && npm run typecheck && npm run build && npm run knip",
    "dev": "next dev",
    "dev:https": "next dev --experimental-https",
    "build": "next build",
    "build:ci": "dotenv -e .env-example -- next build",
    "start": "next start",
    "lint": "eslint . --max-warnings 0",
    "lint:fix": "eslint . --fix",
    "typecheck": "npx tsc -b --noEmit",
    "prettier": "prettier . --write",
    "prettier:check": "prettier . --check",
    "knip": "knip",
    "types:generate": "npx openapi-typescript https://mumble-api-prod-714602723919.europe-west6.run.app/swagger/v1/swagger.json -o src/types/api.d.ts",
    "test": "vitest",
    "e2e:mock": "MUMBLE_API_ENV=mock playwright test"
  },
  "dependencies": {
    "@krrli/cm-designsystem": "1.37.5",
    "better-auth": "1.4.10",
    "dotenv": "^17.2.3",
    "next": "16.1.1",
    "openapi-fetch": "0.15.0",
    "pg": "^8.16.3",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "tailwind-variants": "3.2.2",
    "ulid": "^3.0.2"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "16.1.1",
    "@playwright/test": "^1.57.0",
    "@tailwindcss/postcss": "4",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/node": "24.10.4",
    "@types/pg": "^8.16.0",
    "@types/react": "19.2.7",
    "@types/react-dom": "19.2.3",
    "@typescript-eslint/eslint-plugin": "8.52.0",
    "@typescript-eslint/parser": "8.52.0",
    "@vitejs/plugin-react": "^5.1.2",
    "@vitest/coverage-v8": "^4.0.16",
    "dotenv-cli": "^11.0.0",
    "eslint": "9.39.2",
    "eslint-config-next": "16.1.1",
    "eslint-config-prettier": "10.1.8",
    "jsdom": "^27.4.0",
    "json-server": "^1.0.0-beta.3",
    "knip": "5.80.0",
    "openapi-typescript": "^7.10.1",
    "postcss": "8.5.6",
    "prettier": "3.7.4",
    "prettier-plugin-tailwindcss": "0.7.2",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "4",
    "typescript": "5.9.3",
    "vite-tsconfig-paths": "^6.0.3",
    "vitest": "^4.0.16"
  }
}
