{
  "name": "cm-mumble",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "preflight": "npm run prettier && npm run lint && npm run typecheck && npm run build && npm run knip",
    "dev": "next dev",
    "dev:https": "next dev --experimental-https",
    "build": "next build",
    "build:ci": "dotenv -e .env-example -- next build",
    "start": "next start",
    "lint": "eslint . --max-warnings 0",
    "lint:fix": "eslint . --fix",
    "typecheck": "npx tsc -b --noEmit",
    "prettier": "prettier . --write",
    "prettier:check": "prettier . --check",
    "knip": "knip",
    "types:generate": "npx openapi-typescript https://mumble-api-prod-714602723919.europe-west6.run.app/swagger/v1/swagger.json -o src/types/api.d.ts"
  },
  "dependencies": {
    "better-auth": "1.4.9",
    "@krrli/cm-designsystem": "1.34.7",
    "next": "16.1.1",
    "openapi-fetch": "0.15.0",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "tailwind-variants": "3.2.2",
    "ulid": "^3.0.2"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "16.1.1",
    "@tailwindcss/postcss": "4",
    "@types/node": "24.10.4",
    "@types/react": "19.2.7",
    "@types/react-dom": "19.2.3",
    "@typescript-eslint/eslint-plugin": "8.50.1",
    "@typescript-eslint/parser": "8.50.1",
    "dotenv-cli": "^11.0.0",
    "eslint": "9.39.2",
    "eslint-config-next": "16.1.1",
    "eslint-config-prettier": "10.1.8",
    "knip": "5.77.4",
    "openapi-typescript": "^7.10.1",
    "postcss": "8.5.6",
    "prettier": "3.7.4",
    "prettier-plugin-tailwindcss": "0.7.2",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "4",
    "typescript": "5.9.3"
  }
}
